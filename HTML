<script>
document.getElementById('formInteresse').addEventListener('submit', function(e) {
    e.preventDefault(); // Impede o envio padrão do formulário.

    // Certifique-se de que seu documento HTML esteja com a codificação UTF-8 para suportar emojis.
    var cursoInteresseSelect = document.getElementById('cursoInteresse');
    var cursoInteresse = cursoInteresseSelect.options[cursoInteresseSelect.selectedIndex].text; // Pega o texto selecionado.
    var nome = document.getElementById('nome').value;
    var email = document.getElementById('email').value;
    
    // Substitua pelo seu número de WhatsApp no formato internacional sem o +, por exemplo: 5511912345678.
    var numeroWhatsApp = "5533984284743";

    // Monta a mensagem com emojis.
    var mensagem = encodeURIComponent(
        "Vim pelo site da Microlins GV e quero saber mais sobre o desconto para o curso: " + cursoInteresse + "" +
        ".\n\nMeu nome é: " + nome +
        "\nE-mail: " + email +
        "\n\nAguardo retorno!"
    );

    // Abre a URL do WhatsApp.
    var whatsappUrl = "https://wa.me/" + numeroWhatsApp + "?text=" + mensagem;
    window.open(whatsappUrl, '_blank');
});
</script>



